{
  "name": "dbt-runner",
  "displayName": "RunTable",
  "description": "DBT run a single table",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dbt-runner.run_exact",
        "title": "Run Exact Table",
        "category": "DBT Runner"
      },
      {
        "command": "dbt-runner.run_prepend",
        "title": "Run +Table",
        "category": "DBT Runner"
      },
      {
        "command": "dbt-runner.run_all",
        "title": "Run +Table+",
        "category": "DBT Runner"
      },
      {
        "command": "dbt-runner.run_after",
        "title": "Run Table+",
        "category": "DBT Runner"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "dbt-runner.run_exact",
          "group": "dbt_runner@1",
          "when": "resourceExtname == '.sql'"
        },
        {
          "command": "dbt-runner.run_prepend",
          "group": "dbt_runner@2",
          "when": "resourceExtname == '.sql'"
        },
        {
          "command": "dbt-runner.run_after",
          "group": "dbt_runner@3",
          "when": "resourceExtname == '.sql'"
        },
        {
          "command": "dbt-runner.run_all",
          "group": "dbt_runner@4",
          "when": "resourceExtname == '.sql'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "esbuild": "^0.25.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
